{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress",
    "dashboard"
  ],
  "testRunner": "jest",
  "jest": {
    "projectType": "custom",
    "config": "./jest.config.ts",
    "enableFindRelatedTests": true
  },
  "coverageAnalysis": "perTest",
  "mutate": [
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.spec.ts",
    "!src/types/**",
    "!src/**/__tests__/**",
    "!src/**/__mocks__/**"
  ],
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": 50
  },
  "timeoutMS": 60000,
  "timeoutFactor": 1.5,
  "maxConcurrentTestRunners": 4,
  "concurrency": 4,
  "incremental": true,
  "incrementalFile": ".stryker-tmp/incremental.json",
  "ignorePatterns": [
    "node_modules",
    "dist",
    "coverage",
    "tests",
    ".stryker-tmp",
    "reports"
  ],
  "mutator": {
    "plugins": [
      "@stryker-mutator/typescript-checker"
    ],
    "excludedMutations": []
  },
  "checkers": [
    "typescript"
  ],
  "tsconfigFile": "tsconfig.json",
  "symlinkNodeModules": false,
  "tempDirName": ".stryker-tmp",
  "cleanTempDir": true,
  "htmlReporter": {
    "baseDir": "reports/mutation",
    "fileName": "mutation-report.html"
  },
  "dashboard": {
    "reportType": "full"
  },
  "warnings": {
    "unknownOptions": false,
    "slow": true
  }
}
