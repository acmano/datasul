  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "description": "Connections with circuit breaker OPEN, error rate > 5%, or P95 > 1000ms",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "vertical",
        "reduceOptions": {
          "calcs": [
            "count"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "value_and_name"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "count(db_circuit_breaker_state{state=\"OPEN\"})",
          "legendFormat": "Circuit Breaker OPEN",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "count((sum(rate(db_queries_failed[5m])) by (connection) / sum(rate(db_queries_total[5m])) by (connection)) > 0.05)",
          "legendFormat": "Error Rate > 5%",
          "refId": "B"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "count(histogram_quantile(0.95, sum(rate(db_query_duration_seconds_bucket[5m])) by (le, connection)) > 1)",
          "legendFormat": "P95 > 1000ms",
          "refId": "C"
        }
      ],
      "title": "Critical Connections",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "description": "Connections with error rate 1-5%, P95 500-1000ms, or pool utilization > 85%",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "yellow",
                "value": null
              }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "id": 2,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "vertical",
        "reduceOptions": {
          "calcs": [
            "count"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "value_and_name"
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "count((sum(rate(db_queries_failed[5m])) by (connection) / sum(rate(db_queries_total[5m])) by (connection)) > 0.01 and (sum(rate(db_queries_failed[5m])) by (connection) / sum(rate(db_queries_total[5m])) by (connection)) <= 0.05)",
          "legendFormat": "Error Rate 1-5%",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "count(histogram_quantile(0.95, sum(rate(db_query_duration_seconds_bucket[5m])) by (le, connection)) > 0.5 and histogram_quantile(0.95, sum(rate(db_query_duration_seconds_bucket[5m])) by (le, connection)) <= 1)",
          "legendFormat": "P95 500-1000ms",
          "refId": "B"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "count((db_pool_active_connections / db_pool_max_connections) > 0.85)",
          "legendFormat": "Pool Utilization > 85%",
          "refId": "C"
        }
      ],
      "title": "Degraded Connections",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "description": "SLA compliance: % of queries completed in < 500ms (Target: 99.9%)",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "max": 100,
          "min": 99,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 99.5
              },
              {
                "color": "green",
                "value": 99.9
              }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "id": 3,
      "options": {
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "(sum(rate(db_query_duration_seconds_bucket{le=\"0.5\"}[5m])) / sum(rate(db_query_duration_seconds_count[5m]))) * 100",
          "legendFormat": "SLA Compliance",
          "refId": "A"
        }
      ],
      "title": "SLA Compliance (< 500ms)",
      "type": "gauge"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "description": "Recent alerts from Prometheus alertmanager",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Severity"
            },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background"
                }
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "critical": {
                        "color": "red",
                        "index": 1
                      },
                      "warning": {
                        "color": "yellow",
                        "index": 0
                      }
                    },
                    "type": "value"
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Connection"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 150
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Alert"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 200
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "id": 4,
      "options": {
        "cellHeight": "sm",
        "footer": {
          "countRows": false,
          "fields": "",
          "reducer": [
            "sum"
          ],
          "show": false
        },
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Timestamp"
          }
        ]
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "ALERTS{alertname=~\".*Database.*|.*Connection.*|.*Circuit.*|.*Pool.*\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "Alert History (Last 50)",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Job": true,
              "instance": true,
              "__name__": true
            },
            "indexByName": {
              "Time": 0,
              "alertname": 1,
              "connection": 2,
              "severity": 3,
              "alertstate": 4
            },
            "renameByName": {
              "Time": "Timestamp",
              "alertname": "Alert",
              "alertstate": "State",
              "connection": "Connection",
              "severity": "Severity"
            }
          }
        },
        {
          "id": "limit",
          "options": {
            "limitField": 50
          }
        }
      ],
      "type": "table"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "description": "Connections by health status",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          },
          "mappings": [],
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Healthy"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Degraded"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "yellow",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Critical"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Down"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "dark-red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 18
      },
      "id": 5,
      "options": {
        "displayLabels": [
          "percent",
          "name",
          "value"
        ],
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "values": [
            "value",
            "percent"
          ]
        },
        "pieType": "pie",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "count(db_connection_up == 1) - count((sum(rate(db_queries_failed[5m])) by (connection) / sum(rate(db_queries_total[5m])) by (connection)) > 0.01) - count(db_circuit_breaker_state{state=\"OPEN\"})",
          "legendFormat": "Healthy",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "count((sum(rate(db_queries_failed[5m])) by (connection) / sum(rate(db_queries_total[5m])) by (connection)) > 0.01 and (sum(rate(db_queries_failed[5m])) by (connection) / sum(rate(db_queries_total[5m])) by (connection)) <= 0.05)",
          "legendFormat": "Degraded",
          "refId": "B"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "count((sum(rate(db_queries_failed[5m])) by (connection) / sum(rate(db_queries_total[5m])) by (connection)) > 0.05)",
          "legendFormat": "Critical",
          "refId": "C"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "count(db_connection_up == 0)",
          "legendFormat": "Down",
          "refId": "D"
        }
      ],
      "title": "Connection Health Distribution",
      "type": "piechart"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "description": "Alert firing trend over time",
      "fieldConfig": {
