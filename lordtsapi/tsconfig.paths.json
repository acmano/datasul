{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@api/*": [
        "src/api/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@infrastructure/*": [
        "./src/infrastructure/*"
      ],
      "@shared/*": [
        "./src/shared/*"
      ],
      "@/*": [
        "./src/*"
      ],
      "@item/*": [
        "./src/item/*"
      ],
      "@familia/*": [
        "./src/familia/*"
      ],
      "@grupoDeEstoque/*": [
        "./src/grupoDeEstoque/*"
      ],
      "@familiaComercial/*": [
        "./src/familiaComercial/*"
      ],
      "@estabelecimento/*": [
        "./src/estabelecimento/*"
      ],
      "@datasul/shared-types": [
        "../shared-types/src"
      ],
      "@datasul/shared-types/*": [
        "../shared-types/src/*"
      ]
    }
  }
}